{
  "name": "vue",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "test:unit": "rimraf reports/unit && vitest --coverage --ui -c vitest.config.ts",
    "test:e2e": "rimraf reports/e2e && playwright test && playwright show-report public/reports/e2e",
    "eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "stylelint": "stylelint \"**/*.{css,vue}\" --color --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.": [
      "prettier . --write"
    ],
    "**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "eslint . --ext .ts,.vue --fix"
    ],
    "**/*.{css,vue}": [
      "stylelint \"**/*.{css,vue}\" --color --fix"
    ]
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^5.0.1",
    "@vueuse/components": "^9.13.0",
    "@vueuse/core": "^9.13.0",
    "@vueuse/router": "^9.13.0",
    "shiki": "0.14.1",
    "nanoid": "^4.0.2",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@commitlint/format": "^17.4.4",
    "@faker-js/faker": "^7.6.0",
    "@iconify/vue": "^4.1.1",
    "@pinia/testing": "^0.0.15",
    "@playwright/test": "^1.32.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/d3": "^7.4.0",
    "@types/jsdom": "^21.1.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.29.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "markdown-it-anchor": "^8.6.7",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.3.0",
    "postcss-reporter": "^7.0.5",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "rimraf": "^4.4.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^15.4.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^32.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vitepress": "1.0.0-alpha.65",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0"
  }
}
